            "rawSql": "SELECT\n  $__time(time),\n  CONCAT(benchmark_suite,'/',benchmark_name),\n  -- Following are just for alerting purposes and should be hidden\n  ((metrics->>'throughput')::numeric - (metrics->>'ref_throughput')::numeric + 2 * (metrics->>'stdev_throughput')::numeric) / (metrics->>'ref_throughput')::numeric * 100 as \"2stdev tolerance leeway\",\n  ((metrics->>'throughput')::numeric - (metrics->>'ref_throughput')::numeric) / (metrics->>'ref_throughput')::numeric + (metrics->>'tolerance')::numeric as \"manual tolerance leeway\" \nFROM\n  microbenchmark_results\nWHERE\n  $__timeFilter(time) AND\n  git_branch = 'origin/master' AND\n  wal_device = 'RAM disk'\n  ORDER BY\n  time ASC\n",
